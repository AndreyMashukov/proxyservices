{
    "name": "andrey-mashukov/proxy",
    "keywords": ["proxy", "proxyservices", "proxychecker"],
    "homepage": "https://github.com/AndreyMashukov/proxyservices",
    "description": "Proxyservices - proxy grabber and checker",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Mashukov",
            "email": "a.mashukov@yandex.ru",
            "role": "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "AM\\": ["src"],
            "AM\\ProxyServices\\": ["src", "src/services"],
            "AM\\ProxyServices\\Interfaces\\": ["src/interfaces"]
        }
    },
    "require": {
        "php": ">=7.0",
        "andrey-mashukov/container": "^4.0",
        "andrey-mashukov/http-client-extensions": "^1.0",
        "ext-mbstring": "*",
        "ext-SimpleXML": "*",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-dom": "*"
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\": ["tests"]
        }
    },
    "require-dev": {
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^5.2",
        "logics/phpunit-extensions": "^0.2||dev-trunk",
        "logics/codesniffer": "^0.1||dev-trunk",
        "logics/extcheck": "^0.1||dev-trunk",
        "ext-dom": "*"
    },
    "prefer-stable": true
}